import uuid
import crypto.crc
import .utils

random-name --uuid/uuid.Uuid=random-uuid -> string:
  bytes := uuid.to-byte-array
  cutoff := bytes.size / 2
  adjective := ADJECTIVES[(crc.crc32 bytes[..cutoff]) % ADJECTIVES.size]
  noun := NOUNS[(crc.crc32 bytes[cutoff..]) % NOUNS.size]
  return "$adjective-$noun"

